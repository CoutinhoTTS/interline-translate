{
  "name": "sidecar-translate",
  "displayName": "Sidecar Translate",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "browser": "./out/web-extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sidecar-translate.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "sidecar-translate.whatTimeIsItNow",
        "title": "What time is it now?"
      },
      {
        "command": "sidecar-translate.translateSelectedText",
        "title": "Translate Selected Text"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run build",
    "build": "node scripts/build",
    "compile": "pnpm run build",
    "watch": "pnpm run build -watch",
    "pretest": "pnpm run build && pnpm run lint",
    "serve": "serve -C",
    "lint": "eslint .",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.3.2",
    "axios": "^1.4.0",
    "esbuild": "^0.18.16",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ofetch": "^1.1.1",
    "typescript": "^5.1.3"
  }
}
